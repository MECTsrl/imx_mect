Custom project wizard configuration example file. Note that by convention,
	   the project file goes last.
	   The "class" and "firstpage" attributes specify that it is a Qt 4 wizard and
	   leave room for the Qt 4 target page.
	   -->
	   <wizard version="1" kind="project"
	   class="qt4project" firstpage="10"
	   id="A.customqtproject" category="B.CustomProjects">
	   <icon>console.png</icon>
	   <description>Creates an empty Qt project for TPAC1007_04_AB with the template used by ATCM Applications and by QATCMplugin.</description>
	   <displayname>TPAC1007_04_AB</displayname>;
	   <displaycategory>Mect Projects</displaycategory>
	   <files>
	   <file source="../ATCM-template-project/pages.cpp" target="pages.cpp" openeditor="false"/>
	   <file source="../ATCM-template-project/pages.h" target="pages.h" openeditor="false"/>
	   <file source="../ATCM-template-project/page100.cpp"  target="page100.cpp" openeditor="false"/>
	   <file source="../ATCM-template-project/page100.h"  target="page100.h" openeditor="false"/>
	   <file source="../ATCM-template-project/page100.ui"  target="page100.ui" openeditor="true"/>
	   <file source="../ATCM-template-project/style.h" target="style.h" openproject="false"/>
	   <file source="../ATCM-template-project/automation.cpp" target="automation.cpp" openeditor="false"/>
	   <file source="../ATCM-template-project/automation.h" target="automation.h" openeditor="false"/>
	   <file source="../ATCM-template-project/resources.qrc" target="resources.qrc" openproject="false"/>
	   <file source="../ATCM-template-project/languages.qrc" target="languages.qrc" openproject="false"/>
	   <file source="../ATCM-template-project/config/splash.png" target="config/splash.png" openproject="false" binary="true"/>
	   <file source="config/Crosstable.csv" target="config/Crosstable.csv" openproject="false" binary="false"/>
	   <file source="config/crosstable.cpp" target="config/crosstable.cpp" openproject="false" binary="false"/>
	   <file source="config/crosstable.h" target="config/crosstable.h" openproject="false" binary="false"/>
	   <file source="config/system.ini" target="config/system.ini" openproject="false" binary="false"/>
	   <file source="config/trend1.csv" target="config/trend1.csv" openproject="false" binary="false"/>
	   <!-- 
	   <file source="../ATCM-template-project/project.pro.user" target="%ProjectName%.pro.shared" openproject="false"/>
	   -->
	   <file source="../ATCM-template-project/project.pro" target="%ProjectName%.pro" openproject="true"/>
	   <file source="template.pri" target="template.pri" openproject="false"/>
	   <file source="../ATCM-template-project/common.pri" target="common.pri" openproject="false"/>
	   <file source="../ATCM-template-project/languages.pri" target="languages.pri" openproject="false"/>
	   <file source="../ATCM-template-project/systemicons.qrc" target="systemicons.qrc" openproject="false"/>
	   </files>
	    <!-- Create a 2nd wizard page with parameters -->
	    <fieldpagetitle>Add extra funtionality</fieldpagetitle>
		<fields>
			<field name="Model">
				<fieldcontrol class="QComboBox" defaultindex="2">
					<comboentries>
						<comboentry value="&lt;width&gt;480&lt;&#47;width&gt;&lt;height&gt;272&lt;&#47;height&gt;">
							<comboentrytext>Horizontal</comboentrytext>
						</comboentry>
						<comboentry value="&lt;width&gt;272&lt;&#47;width&gt;&lt;height&gt;480&lt;&#47;height&gt;">
							<comboentrytext>Vertical</comboentrytext>
						</comboentry>
				</comboentries>
				</fieldcontrol>
				<fielddescription>Choose the panel orientation:</fielddescription>
			</field>
		</fields>
	</wizard>
