<externaltool id="Crosstable_editor">
    <description>This tool open the Crosstable</description>
    <displayname>Crosstable Editor</displayname>
    <category>Mect Configurator</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>C:/Program Files/MectConfiguratorInstaller/MectConfigurator.exe</path>
        <arguments>--Crosstable %{CurrentProject:Path}/config/Crosstable.csv</arguments>
        <workingdirectory>%{CurrentProject:Path}/config</workingdirectory>
    </executable>
</externaltool>
