<externaltool id="Settings_Editor">
    <description>This tool open the system.ini file</description>
    <displayname>System Editor</displayname>
    <category>Mect Configurator</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>C:/Qt485/desktop/bin/SystemConf.exe</path>
        <arguments>%{CurrentProject:Path}</arguments>
        <workingdirectory>%{CurrentProject:Path}/config</workingdirectory>
    </executable>
</externaltool>
