<externaltool id="lupdate">
    <description>Synchronizes translator's ts files with the program code</description>
    <displayname>Update Translations (lupdate)</displayname>
    <category>Linguist</category>
    <order>1</order>
    <executable output="showinpane" error="showinpane" modifiesdocument="no">
        <path>C:/Qt485/desktop/bin/lupdate.exe</path>
        <path>lupdate</path>
        <arguments>%{CurrentProject:FilePath}</arguments>
        <workingdirectory>%{CurrentProject:Path}</workingdirectory>
    </executable>
</externaltool>
