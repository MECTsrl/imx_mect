+#!/bin/sh
+
+test -x /sbin/mdev || exit 0
+
+case "$1" in
+    start)
+        if ! grep -q 'devtmpfs /dev devtmpfs ' /proc/mounts; then
+            # put /dev in a tmpfs
+            mount -n -o mode=0755 -t tmpfs mdev /dev
+
+            # Create static device nodes in /dev
+            mknod /dev/console c 5 1
+            chmod 600 /dev/console
+            mknod /dev/null c 1 3
+            chmod 666 /dev/null
+
+            # make and mount devpts
+            mkdir /dev/pts
+            mount -n -t devpts devpts /dev/pts
+
+            echo "Starting the hotplug events dispatcher mdev"
+            /sbin/mdev -s
+
+            mkdir /dev/shm
+        fi
+
+        echo "/sbin/mdev" > /proc/sys/kernel/hotplug
+
+        for c in '' $(find /sys/devices/platform -name bConfigurationValue -print); do
+            test -n "$c" || continue
+
+            test -w "$c" || continue
+
+            v="$(cat $c 2> /dev/null)"
+            test -n "$v" || v=1
+
+            echo   -1 > "$c"
+            echo "$v" > "$c"
+        done
+        ;;
+
+    stop)
+        ;;
+
+    *)
+        echo "Usage: /etc/rc.d/init.d/mdev {start|stop}"
+        echo
+        exit 1
+        ;;
+esac
+
+exit 0
