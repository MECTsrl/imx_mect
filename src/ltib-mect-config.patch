+#!/bin/sh
+
+VPNSRVIP='10.1.0.1'
+VPNSRVPORT='8080'
+
+TUNNAME='tun_mrs'
+
+PIDD=/var/run
+PIDFILE="$PIDD/$(/usr/bin/basename $0).pid"
+
+action='start'
+test -n "$1" && action="$1"
+
+# Start the push unless already running.
+start() {
+	# Abort if already running.
+	test -r "$PIDFILE" && test -s "$PIDFILE" && test -d /proc/$(/bin/cat "$PIDFILE") && return 0
+
+        # Abort if no VPN or if unexpected VPN peer IP.
+        /sbin/ip addr show "$TUNNAME" 2>/dev/null | /bin/grep -q "\<peer ${VPNSRVIP}/32 scope global ${TUNNAME}\$" || return 0
+
+        # OK then, let's do our job!
+	echo $$ > "$PIDFILE"
+
+	# Try to notify four times in case of errors.
+	for n in $(/usr/bin/seq 1 1 4); do
+		msg="$(/usr/bin/wget -q -O - http://${VPNSRVIP}:${VPNSRVPORT}/log_sync.php?newalarm)"
+		rv=$?
+
+		# Succeeded.
+		test $rv -eq 0 && break
+
+		# No luck yet, try again.
+		sleep 30
+	done
+
+	/bin/rm -f "$PIDFILE"
+
+	return $rv
+}
+
+# Stop an already running push.
+stop() {
+	# Stop nicely.
+	for n in $(/usr/bin/seq 1 1 3); do
+		if test -r "$PIDFILE" && test -s "$PIDFILE" && test -d /proc/$(/bin/cat "$PIDFILE"); then
+			kill $(cat "$PIDFILE")
+		else
+			/bin/rm -f "$PIDFILE"
+
+			break
+		fi
+
+		sleep 3
+	done
+
+	# Forcibly stop.
+	for n in $(/usr/bin/seq 1 1 3); do
+		if test -r "$PIDFILE" && test -s "$PIDFILE" && test -d /proc/$(/bin/cat "$PIDFILE"); then
+			kill -9 $(cat "$PIDFILE")
+		else
+			/bin/rm -f "$PIDFILE"
+
+			break
+		fi
+
+		sleep 2
+	done
+
+	# Any luck?
+	if test -r "$PIDFILE" && test -s "$PIDFILE" && test -d /proc/$(/bin/cat "$PIDFILE"); then
+		return 1
+	else
+		return 0
+	fi
+}
+
+case "$action" in
+	'start')
+		start
+	;;
+
+	'stop')
+		stop
+	;;
+
+	'restart')
+		stop
+		start
+	;;
+
+	*)
+		echo "Usage: $(basename $0) [start|stop|restart]"
+
+        
+	;;
+esac
