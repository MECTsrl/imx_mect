# Target TP1043_485

TP1043_485%: MECT_SUFFIX := TP1043_485
TP1043_485%: MECT_TGTDIR := $(MECT_IMGDIR)/TP1043_485_ms$(MECT_BUILD_RELEASE)
TP1043_485%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TP1043_485%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TP1043_485%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TP1043_485%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TP1043_485%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TP1043_485%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TP1043_485%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1007_480x272.config

.PHONY: TP1043_485
TP1043_485: TP1043_485_mfg

.PHONY: TP1043_485_boot
TP1043_485_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TP1043_485_rfs
TP1043_485_rfs:
	test -n '$(MECT_BUILD_TARGET_TP1043_485)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TP1043_485)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TP1043_485_lfs
TP1043_485_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TP1043_485_mfg
TP1043_485_mfg: TP1043_485_boot TP1043_485_rfs TP1043_485_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
