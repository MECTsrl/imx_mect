# Target TP1057

.PHONY: TP1057
TP1057: MECT_BUILD_TARGET := TP1057
TP1057: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1006_320x240.config
TP1057: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_BUILD_TARGET)-,$(MECT_LTIB_KERNEL_RPM))
TP1057: MECT_TGTDIR = $(MECT_IMGDIR)/$(MECT_BUILD_TARGET)_ms$(MECT_BUILD_RELEASE)
TP1057: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TP1057: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TP1057: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TP1057: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TP1057: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TP1057:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' MECT_LFSPKGS=$(MECT_TARGET_LFSPKG_ATCMCONTROL_RUNTIMESYSTEM) target_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
