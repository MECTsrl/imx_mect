# Target TPAC1006

TPAC1006%: MECT_SUFFIX := TPAC1006
TPAC1006%: MECT_TGTDIR := $(MECT_IMGDIR)/TPAC1006_ms$(MECT_BUILD_RELEASE)
TPAC1006%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TPAC1006%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TPAC1006%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TPAC1006%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TPAC1006%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TPAC1006%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TPAC1006%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1006_320x240.config

.PHONY: TPAC1006
TPAC1006: TPAC1006_mfg

.PHONY: TPAC1006_boot
TPAC1006_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TPAC1006_rfs
TPAC1006_rfs:
	test -n '$(MECT_BUILD_TARGET_TPAC1006)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TPAC1006)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TPAC1006_lfs
TPAC1006_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TPAC1006_mfg
TPAC1006_mfg: TPAC1006_boot TPAC1006_rfs TPAC1006_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
