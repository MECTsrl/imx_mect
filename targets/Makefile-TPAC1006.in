# Target TPAC1006

.PHONY: TPAC1006
TPAC1006: MECT_BUILD_TARGET := TPAC1006
TPAC1006: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1006_320x240.config
TPAC1006:
	test -r $(MECT_TARGET_LFSPKG_4C_RUNTIME)
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' target_boot
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' target_rfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_LFSPKGS=$(MECT_TARGET_LFSPKG_4C_RUNTIME) target_lfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' target_mfg
