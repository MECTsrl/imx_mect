# Target TPAC1006_HR

TPAC1006_HR%: MECT_SUFFIX := TPAC1006_HR
TPAC1006_HR%: MECT_TGTDIR := $(MECT_IMGDIR)/TPAC1006_HR_ms$(MECT_BUILD_RELEASE)
TPAC1006_HR%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TPAC1006_HR%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TPAC1006_HR%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TPAC1006_HR%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TPAC1006_HR%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TPAC1006_HR%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TPAC1006_HR%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1006_640x480.config

.PHONY: TPAC1006_HR
TPAC1006_HR: TPAC1006_HR_mfg

.PHONY: TPAC1006_HR_boot
TPAC1006_HR_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TPAC1006_HR_rfs
TPAC1006_HR_rfs:
	test -n '$(MECT_BUILD_TARGET_TPAC1006_HR)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TPAC1006_HR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TPAC1006_HR_lfs
TPAC1006_HR_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TPAC1006_HR_mfg
TPAC1006_HR_mfg: TPAC1006_HR_boot TPAC1006_HR_rfs TPAC1006_HR_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
