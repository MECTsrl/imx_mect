# Target TPAC1007_4AA

TPAC1007_4AA%: MECT_SUFFIX := TPAC1007_4AA
TPAC1007_4AA%: MECT_TGTDIR := $(MECT_IMGDIR)/TPAC1007_4AA_ms$(MECT_BUILD_RELEASE)
TPAC1007_4AA%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TPAC1007_4AA%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TPAC1007_4AA%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TPAC1007_4AA%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TPAC1007_4AA%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TPAC1007_4AA%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TPAC1007_4AA%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1007_480x272.config

.PHONY: TPAC1007_4AA
TPAC1007_4AA: TPAC1007_4AA_mfg

.PHONY: TPAC1007_4AA_boot
TPAC1007_4AA_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TPAC1007_4AA_rfs
TPAC1007_4AA_rfs:
	test -n '$(MECT_BUILD_TARGET_TPAC1007_4AA)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TPAC1007_4AA)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TPAC1007_4AA_lfs
TPAC1007_4AA_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TPAC1007_4AA_mfg
TPAC1007_4AA_mfg: TPAC1007_4AA_boot TPAC1007_4AA_rfs TPAC1007_4AA_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
