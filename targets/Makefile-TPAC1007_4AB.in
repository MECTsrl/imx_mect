# Target TPAC1007_4AB

TPAC1007_4AB%: MECT_SUFFIX := TPAC1007_4AB
TPAC1007_4AB%: MECT_TGTDIR := $(MECT_IMGDIR)/TPAC1007_4AB_ms$(MECT_BUILD_RELEASE)
TPAC1007_4AB%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TPAC1007_4AB%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TPAC1007_4AB%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TPAC1007_4AB%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TPAC1007_4AB%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TPAC1007_4AB%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TPAC1007_4AB%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1007_480x272.config

.PHONY: TPAC1007_4AB
TPAC1007_4AB: TPAC1007_4AB_mfg

.PHONY: TPAC1007_4AB_boot
TPAC1007_4AB_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TPAC1007_4AB_rfs
TPAC1007_4AB_rfs:
	test -n '$(MECT_BUILD_TARGET_TPAC1007_4AB)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TPAC1007_4AB)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TPAC1007_4AB_lfs
TPAC1007_4AB_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TPAC1007_4AB_mfg
TPAC1007_4AB_mfg: TPAC1007_4AB_boot TPAC1007_4AB_rfs TPAC1007_4AB_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
