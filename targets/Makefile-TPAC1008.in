# Target TPAC1008

TPAC1008%: MECT_SUFFIX := TPAC1008
TPAC1008%: MECT_TGTDIR := $(MECT_IMGDIR)/TPAC1008_ms$(MECT_BUILD_RELEASE)
TPAC1008%: MECT_MFGDIR = $(MECT_TGTDIR)/$(shell basename $(MECT_TGTDIR) | sed 's/\./_/g')
TPAC1008%: MECT_MFGZIP = $(MECT_MFGDIR)/../$(shell basename $(MECT_MFGDIR)).zip
TPAC1008%: MECT_BOOTDIR = $(MECT_TGTDIR)/boot
TPAC1008%: MECT_RFSDIR = $(MECT_TGTDIR)/rootfs
TPAC1008%: MECT_LFSDIR = $(MECT_TGTDIR)/localfs
TPAC1008%: MECT_KERNELRPM = $(subst /kernel-,/kernel-rfs-$(MECT_TARGET_PREFIX)$(MECT_SUFFIX)-,$(MECT_LTIB_KERNEL_RPM))
TPAC1008%: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1008_800x480.config

.PHONY: TPAC1008
TPAC1008: TPAC1008_mfg

.PHONY: TPAC1008_boot
TPAC1008_boot:
	$(MAKE) MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' target_boot

.PHONY: TPAC1008_rfs
TPAC1008_rfs:
	test -n '$(MECT_BUILD_TARGET_TPAC1008)'
	$(MAKE) BUILD_TARGET='$(MECT_BUILD_TARGET_TPAC1008)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_SUFFIX='$(MECT_SUFFIX)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' MECT_KERNELRPM='$(MECT_KERNELRPM)' MECT_RFSDIR='$(MECT_RFSDIR)' target_rfs

.PHONY: TPAC1008_lfs
TPAC1008_lfs:
	$(MAKE) MECT_LFSDIR='$(MECT_LFSDIR)' target_lfs

.PHONY: TPAC1008_mfg
TPAC1008_mfg: TPAC1008_boot TPAC1008_rfs TPAC1008_lfs
	$(MAKE) MECT_MFGDIR='$(MECT_MFGDIR)' MECT_RFSDIR='$(MECT_RFSDIR)' MECT_LFSDIR='$(MECT_LFSDIR)' MECT_TGTDIR='$(MECT_TGTDIR)' MECT_BOOTDIR='$(MECT_BOOTDIR)' MECT_MFGZIP='$(MECT_MFGZIP)' target_mfg
