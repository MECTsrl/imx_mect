# Target TPAC1008

.PHONY: TPAC1008
TPAC1008: MECT_BUILD_TARGET := TPAC1008
TPAC1008: MECT_KERNEL_TARGET_CONF := kernel-2.6.35-imx28-tpac1008_800x480.config
TPAC1008: MECT_LFSPKGS := \
		$(MECT_TARGET_LFSPKG_4C_RUNTIME) \
		$(MECT_RPMDIR)/mect_apps-lfs-$(MECT_BUILD_TARGET)-$(MECT_BUILD_APPSCRT_TAG)-1.arm.rpm
TPAC1008:
	test -r $(MECT_TARGET_LFSPKG_4C_RUNTIME)
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' target_boot
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' target_rfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_LFSPKGS=$(MECT_LFSPKGS) target_lfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' target_mfg
