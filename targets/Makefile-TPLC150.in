# Target TPLC150

.PHONY: TPLC150
TPLC150: MECT_BUILD_TARGET := TPLC150
TPLC150: MECT_KERNEL_TARGET_CONF := kernel-2.6.35.3-imx28-tpac1007_480x272.config
TPLC150:
	test -r $(MECT_TARGET_LFSPKG_4C_RUNTIME)
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_KERNEL_TARGET_CONF='$(MECT_KERNEL_TARGET_CONF)' target_boot target_rfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' MECT_LFSPKGS='$(MECT_TARGET_LFSPKG_4C_RUNTIME)' target_lfs
	$(MAKE) MECT_BUILD_TARGET='$(MECT_BUILD_TARGET)' target_mfg_upd
